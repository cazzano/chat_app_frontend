{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "e_commerce",
  "version": "0.1.0",
  "identifier": "com.tauri.lol",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:9002",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "E-Commerce",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
